{
    "name": "TestCI",
    "description": "Continuous Integration test",
    "config": {
        "platform": {
            "php": "5.4"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
    },
    "require-dev": {
        "phpunit/phpunit": "4.8"
    }
}
